{
    "name": "weew/url-matcher",
    "description": "Simple url matcher and parser.",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Kott",
            "email": "maximkott@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kaivladimirv/collections"
        }
    ],
    "require": {
        "php": ">=8.3",
        "weew/helpers-string": "^1.2",
        "weew/helpers-array": "^1.0",
        "weew/collections": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "satooshi/php-coveralls": "^0.6.1",
        "vimeo/psalm": "^5.26.1"
    }
}
